			<column name="c_first_sales_date_sk" data-category="sk"></column>
			<column name="c_salutation" data-category="nick_name"></column>
			<column name="c_first_name" data-category="first_name"></column>
			<column name="c_last_name" data-category="last_name"></column>
			<column name="c_birth_day" data-category="birth_day"></column>
			<column name="c_birth_month" data-category="birth_month"></column>
			<column name="c_birth_year" data-category="birth_year"></column>
			<column name="c_birth_country" data-category="birth_country"></column>
			<column name="c_login" data-category="login"></column>
			<column name="c_email_address" data-category="email"></column>
		</table>

		<table name="customer_address">
			<column name="ca_address_sk" data-category="sk"></column>
			<column name="ca_address_id" data-category="sk"></column>
			<column name="ca_street_number" data-category="street_number"></column>
			<column name="ca_street_name" data-category="street_name"></column>
			<column name="ca_street_type" data-category="street_type"></column>
			<column name="ca_suite_number" data-category="suite"></column>
			<column name="ca_city" data-category="city"></column>
			<column name="ca_county" data-category="county"></column>
			<column name="ca_state" data-category="state"></column>
			<column name="ca_zip" data-category="zip"></column>
			<column name="ca_country" data-category="country"></column>
			<column name="ca_gmt_offset" data-category="gmt"></column>
			<column name="ca_location_type" data-category="location_type"></column>
		</table>

		<table name="customer_demographics">
			<column name="cd_demo_sk" data-category="sk"></column>
			<column name="cd_gender" data-category="gender"></column>
			<column name="cd_marital_status" data-category="marital"></column>
			<column name="cd_education_status" data-category="education"></column>
			<column name="cd_purchase_estimate" data-category="buy_potential"></column>
			<column name="cd_credit_rating" data-category="credit_rating"></column>
			<column name="cd_dep_count" data-category="dep"></column>
			<column name="cd_dep_employed_count" data-category="dep"></column>
			<column name="cd_dep_college_count" data-category="dep"></column>
		</table>

		<table name="household_demographics">
			<column name="hd_demo_sk" data-category="sk"></column>
			<column name="hd_income_band_sk" data-category="sk"></column>
			<column name="hd_buy_potential" data-category="buy_potential"></column>
			<column name="hd_dep_count" data-category="dep"></column>
			<column name="hd_vehicle_count" data-category="vehicle"></column>
		</table>

		<table name="income_band">
			<column name="ib_income_band_sk" data-category="financial"></column>
			<column name="ib_lower_bound" data-category="financial"></column>
			<column name="ib_upper_bound" data-category="financial"></column>
		</table>
	</database>
</meta-mapping>
