<?xml version="1.0" encoding="UTF-8"?>
<meta-mapping xmlns="http://www.example.org/privacy"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" policy=""
	xsi:schemaLocation="http://www.example.org/privacy ../res/meta.xsd">
	<database name="tpc">
		<table name="customer">
			<column name="c_email_address" data-category="ka"></column>
			<column name="c_first_name" data-category="name"></column>
			<column name="c_last_name" data-category="name"></column>
		</table>
		<table name="customer_demographics">
			<column name="cd_credit_rating" data-category="financial">
				<desensitize-operation name="avg">
					<UDF>avg</UDF>
				</desensitize-operation>
			</column>
			<column name="cd_dep_count" data-category="financial">
				<desensitize-operation name="sum">
					<UDF>sum</UDF>
				</desensitize-operation>
			</column>
		</table>
	</database>
</meta-mapping>
